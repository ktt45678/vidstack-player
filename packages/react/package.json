{
  "name": "@ktt45678/vidstack-react",
  "version": "0.6.13-F1",
  "description": "Build awesome media experiences on the web.",
  "license": "MIT",
  "type": "module",
  "types": "dist/types/index.d.ts",
  "module": "dist/prod.js",
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "files": [
    "*.d.ts",
    "dist/"
  ],
  "scripts": {
    "dev": "pnpm clean && rollup -c -w & pnpm run types -w",
    "build": "node .scripts/prebuild.js && rollup -c",
    "types": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "format": "prettier src --write --loglevel warn"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0",
    "maverick.js": "0.37.0",
    "react": "^18.0.0",
    "vidstack": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "esbuild": "^0.16.0",
    "maverick.js": "0.37.0",
    "media-icons": "0.10.0",
    "react": "^18.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^3.25.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.0.0",
    "vidstack": "workspace:*"
  },
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vidstack/vidstack.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/vidstack/vidstack/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": "./dist/server/index.js",
      "deno": "./dist/server/index.js",
      "development": "./dist/dev/index.js",
      "default": "./dist/prod/index.js"
    },
    "./icons": {
      "types": "./dist/types/icons.d.ts",
      "node": "./dist/server/icons.js",
      "deno": "./dist/server/icons.js",
      "development": "./dist/dev/icons.js",
      "default": "./dist/prod/icons.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vidstack",
    "player",
    "react"
  ]
}
